{
  "name": "__MSG_appName__",
  "version": "1.0.4",
  "description": "__MSG_appDesc__",
  "manifest_version": 3,
  "default_locale": "pt_BR",
  "author": "raphaelbarbosa.rodrigues@gmail.com",
  "icons": {
    "16": "images/vtex-16x16.png",
    "32": "images/vtex-32x32.png",
    "48": "images/vtex-48x48.png",
    "128": "images/vtex-128x128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/vtex-48x48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["cookies", "tabs", "activeTab", "scripting"],
  "host_permissions": ["*://*/*"]
}
